{
  "line-event.yml": {
    "required": [
      {
        "key": "SUPABASE_URL",
        "description": "Supabase REST endpoint (vars)",
        "location": "variable",
        "parameter": "SUPABASE_URL"
      },
      {
        "key": "SUPABASE_SERVICE_ROLE_KEY",
        "description": "Supabase service role key (secret)",
        "location": "secret",
        "parameter": "SUPABASE_SERVICE_ROLE_KEY"
      },
      {
        "key": "GOOGLE_SERVICE_ACCOUNT_JSON",
        "description": "Google service account JSON with Sheets access",
        "location": "secret",
        "parameter": "GOOGLE_SERVICE_ACCOUNT_JSON"
      },
      {
        "key": "GOOGLE_SHEET_ID",
        "description": "Ledger spreadsheet ID",
        "location": "secret",
        "parameter": "GOOGLE_SHEET_ID"
      },
      {
        "key": "MANUS_API_KEY",
        "description": "Manus API key for task dispatch",
        "location": "secret",
        "parameter": "MANUS_API_KEY"
      },
      {
        "key": "MANUS_BASE_URL",
        "description": "Manus base URL",
        "location": "variable",
        "parameter": "MANUS_BASE_URL"
      },
      {
        "key": "GEMINI_API_KEY",
        "description": "Gemini API key used for log summarization",
        "location": "secret",
        "parameter": "GEMINI_API_KEY"
      },
      {
        "key": "LINE_CHANNEL_ACCESS_TOKEN",
        "description": "LINE Messaging API channel access token for reply dispatch",
        "location": "secret",
        "parameter": "LINE_CHANNEL_ACCESS_TOKEN"
      }
    ],
    "alternatives": [
      [
        {
          "key": "NOTIFY_WEBHOOK_URL",
          "description": "Primary stakeholder notification webhook",
          "location": "secret",
          "parameter": "NOTIFY_WEBHOOK_URL"
        },
        {
          "key": "PROGRESS_WEBHOOK_URL",
          "description": "Fallback notification webhook",
          "location": "secret",
          "parameter": "PROGRESS_WEBHOOK_URL"
        }
      ]
    ]
  },
  "manus-progress.yml": {
    "required": [
      {
        "key": "SUPABASE_URL",
        "description": "Supabase REST endpoint (vars)",
        "location": "variable",
        "parameter": "SUPABASE_URL"
      },
      {
        "key": "SUPABASE_SERVICE_ROLE_KEY",
        "description": "Supabase service role key (secret)",
        "location": "secret",
        "parameter": "SUPABASE_SERVICE_ROLE_KEY"
      }
    ]
  }
}
